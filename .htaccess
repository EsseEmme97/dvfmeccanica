RewriteEngine On
#RewriteCond %{HTTPS} off [OR]
#RewriteCond %{HTTP_HOST} ^www\. [NC]
#RewriteCond %{HTTP_HOST} ^(?:www\.)?(.+)$ [NC]
#RewriteRule ^ https://%1%{REQUEST_URI} [L,NE,R=301]

RewriteRule  ^it$ index.php [L]
RewriteRule ^azienda$ it/azienda.php [L]
RewriteRule ^qualita$ it/qualita.php [L]
RewriteRule ^realizzazioni$ it/realizzazioni.php [L]
RewriteRule ^parco-macchine$ it/parco-macchine.php [L]
RewriteRule ^motorsport$ it/motorsport.php [L]
RewriteRule ^lavora-con-noi$ it/lavora-con-noi.php [L]
RewriteRule ^contatti$ it/contatti.php [L]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^.*$ /it/404.html [L]